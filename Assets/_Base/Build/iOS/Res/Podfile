#use_frameworks!
platform :ios, '9.0'
source 'https://github.com/CocoaPods/Specs.git'
target 'Unity-iPhone' do

    # Log
    pod 'CocoaLumberjack'

    # Firebase Analytics
    pod 'Firebase/Core', '~> 5.14.0'
    pod 'Firebase/Analytics', '~> 5.14.0'

    # Firebase Crashlytics
    pod 'Fabric', '~> 1.9.0'
    pod 'Crashlytics', '~> 3.12.0'

    # Bugly
    pod 'Bugly', '~> 2.5.0'

    # AppsFlyer
    pod 'AppsFlyerFramework', '~> 4.8.6'

    # Facebook
    pod 'FBSDKCoreKit', '~> 4.37.0'
end
